<% provide(:title, @user.name) %>
<div class="row">
	<aside class="col-md-4">
		<section class="user_infor">
			<h1>
				<%= gravatar_for @user %>
				<%=@user.name %>
			</h1>
		</section>
	</aside>
</div>

<div>
	<br/>
	<span>Created At</span><%=@user.created_at %>
	<br/>
	<span>Updated At</span><%=@user.updated_at %>
	<br/>
	<span>Time now</span><%= Time.now%>
</div>